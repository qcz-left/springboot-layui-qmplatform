<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qcz.qmplatform.module.operation.mapper.DictAttrMapper">

    <select id="queryDictAttrListByCode" resultType="java.util.Map">
        select attr_value as value, attr_name as name from v_sys_dict_attr where dict_code = #code#
    </select>

</mapper>
