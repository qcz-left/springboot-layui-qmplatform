<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qcz.qmplatform.module.business.system.mapper.ThirdpartyAppMapper">

    <select id="manageBindAccountList"
            resultType="com.qcz.qmplatform.module.business.system.domain.vo.ManageBindAccountVO">
        SELECT
            thirdparty_id,
            sut.access_type as "appName",
            su.username as "accountName"
        FROM
            sys_user_thirdparty sut
        LEFT JOIN sys_user su ON su.ID = sut.user_id
    </select>
</mapper>
